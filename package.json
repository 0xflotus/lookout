{
    "name": "lookout",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*",
        "!apps/supabase"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "supabase": "^1.26.9",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@1.22.19"
}
